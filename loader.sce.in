mode(-1);


// specific part
libname='libsivp'
libtitle='SIVP - Scilab Image Processing Toolbox @PACKAGE_VERSION@';

write(%io(2), libtitle);

libsivp_path=get_file_path('loader.sce');

functions=[@TOOLBOX_FUNCTIONS@];

addinter(libsivp_path+'/lib/libsivp.so','libsivp',functions);

write(%io(2),'--------------------------------------------');
write(%io(2), libtitle + ' loaded');

clear libname;
clear libtitle;