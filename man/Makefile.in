	    echo " $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(helpdir)/$$p"; \
	    $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(helpdir)/$$p; \
	  else if test -f $$p; then \
	    echo " $(INSTALL_DATA) $$p $(DESTDIR)$(helpdir)/$$p"; \
	    $(INSTALL_DATA) $$p $(DESTDIR)$(helpdir)/$$p; \
	  fi; fi; \
	done

